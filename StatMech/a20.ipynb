   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "beta = 0.5\n",
    "def boltz(E):\n",
    "    Z = 1+3*np.exp(-beta)+6*np.exp(-2*beta)+7*np.exp(-3*beta)+6*np.exp(-4*beta)+3*np.exp(-5*beta)+np.exp(-6*beta)\n",
    "    return np.exp(-beta*E)/Z\n",
    "\n",
    "E = np.arange(0,7,1)\n",
    "plt.scatter(E,boltz(E))\n",
    "sum(boltz(E))"
   ]
