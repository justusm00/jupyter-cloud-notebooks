    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "x = np.linspace(0,10,100)\n",
    "\n",
    "def stepfunc(x):\n",
    "    y = np.piecewise(x,[x < 5, x > 5],[0,1])\n",
    "    return y\n",
    "\n",
    "plt.plot(x,stepfunc(x))\n",
    "    "
